@page
@model Restaurant_management_system.Pages.AdminPageModel
@{
}
<p>Welcome to Admin page. </p>
<a class="btn btn-primary" href="/Registration">Add || Remove || Update user</a>
<br>
<br>
<h></h>
<table>
    <thead>

        <tr>
            <th>customer_id || </th>
            <th>customer_name ||  </th>
            <th>customer_email || </th>
            <th>customer_password  </th>            
        </tr>
    </thead>
    <tbody>
        @foreach (var x in Model.list_name)
        {
            <tr>
                <td>@x.c_id </td>
                <td>@x.c_name </td>
                <td>@x.c_email </td>
                <td>@x.c_pass </td>

                <td><a href="/UpdateDetails?id=@x.c_id" class="btn btn-success"> Update</a></td>
            </tr>
        }
    </tbody>
</table>